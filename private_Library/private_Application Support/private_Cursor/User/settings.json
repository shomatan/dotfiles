    "[dart]": {
        "editor.formatOnSave": true,
        "editor.formatOnType": true,
        "editor.rulers": [
            80
        ],
        "editor.selectionHighlight": false,
        "editor.suggestSelection": "first",
        "editor.tabCompletion": "onlySnippets",
        "editor.wordBasedSuggestions": "off"
    },
    "dart.openDevTools": "flutter",
    "cursor.cpp.disabledLanguages": [
        "markdown",
        "scminput"
    ],
    "files.exclude": {
        "**/.trunk/*actions/": true,
        "**/.trunk/*logs/": true,
        "**/.trunk/*notifications/": true,
        "**/.trunk/*out/": true,
        "**/.trunk/*plugins/": true
    },
    "workbench.colorCustomizations": {
        "sideBar.background": "#2B2B2B", // サイドバーの背景色
        "sideBar.foreground": "#FFFFFF", // テキスト色
        "sideBarSectionHeader.background": "#3C3F41", // セクションヘッダーの背景色
        "sideBarSectionHeader.foreground": "#FFC107", // セクションヘッダーのテキスト色
        "activityBar.background": "#2B2B2B", // アクティビティバーの背景色
        "activityBar.foreground": "#FFFFFF", // アクティビティバーのテキスト色
        "activityBar.activeBorder": "#FFC107", // アクティビティバーのアクティブな境界線色
        "editor.foreground": "#FFFFFF", // エディタのテキスト色
        "editor.background": "#2B2B2B", // エディタの背景色
        "editorCursor.foreground": "#FFC107", // カーソルの色
        "editor.lineHighlightBackground": "#3C3F41", // 行ハイライトの背景色
        "editor.selectionBackground": "#4A4A4A", // 選択時の背景色
        "editor.selectionForeground": "#FFFFFF", // 選択時のテキスト色
        "editor.wordHighlightBackground": "#3C3F41", // 語のハイライト背景色
        "editor.wordHighlightBorder": "#4d594d", // 語のハイライト境界線の色
        "editor.findMatchBackground": "#ddd0a8", // 検索一致の背景色
        "editor.findMatchHighlightBackground": "#4A4A4A", // 検索の一致ハイライト背景色
        "editor.hoverHighlightBackground": "#4A4A4A", // ホバー時のハイライト背景色
    },
    "editor.mouseWheelZoom": true, // マウスホイールズームを有効化
    "workbench.colorTheme": "Webstorm IntelliJ Darcula Theme",
    "workbench.activityBar.orientation": "vertical",
    "files.autoSave": "afterDelay",
    "workbench.tree.expandMode": "doubleClick",
    "editor.fontSize": 17,
    "window.zoomLevel": -0.1
}
